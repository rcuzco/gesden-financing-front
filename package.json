{
  "name": "gesden-financing-front",
  "version": "1.0.11-SNAPSHOT",
  "license": "MIT",
  "publishConfig": {"registry": "http://ic.sanitas.dom/nexus/repository/sanitas-npm-private/"},
  "scripts":   {
    "ng": "ng",
    "start:hmr": "ng serve --hmr -e=hmr --open",
    "start": "ng serve --open",
    "start:aot": "ng serve --open --aot",
    "start:mocks": "concurrently --kill-others \"npm run start\" \"npm run mocks\"",
    "pree2e": "webdriver-manager update --standalone false --gecko false --quiet",
    "test": "npm run test:coverage",
    "test:coverage": "ng test --watch=false --code-coverage=true",
    "test:watch": "ng test --code-coverage=true",
    "lint": "ng lint && npm run lint:css",
    "lint:css": "stylelint \"src/**/*.scss\" --config .stylelintrc",
    "e2e": "npm run pree2e && ng e2e",
    "clean": "rimraf dist",
    "build:prod": "npm run clean && ng build --prod --vendor-chunk=true",
    "server:prod": "npm run build:prod && http-server dist/app -c-1 --cors",
    "server:prod:jit": "ng build --prod --aot=false --build-optimizer=false && http-server dist/app -c-1 --cors",
    "documentation": "./node_modules/.bin/compodoc -p tsconfig.json -d documentation/ -s -o ",
    "mocks": "node ./mock_server/index.js",
    "precommit": "npm run lint && npm run test && npm run e2e && npm run server:prod"
  },
  "dependencies":   {
    "@angular/animations": "8.2.0",
    "@angular/common": "8.2.0",
    "@angular/compiler": "8.2.0",
    "@angular/core": "8.2.0",
    "@angular/forms": "8.2.0",
    "@angular/platform-browser": "8.2.0",
    "@angular/platform-browser-dynamic": "8.2.0",
    "@angular/router": "8.2.0",
    "@angularclass/hmr": "2.1.3",
    "@sbc/components": "0.20.1",
    "@ngx-translate/core": "11.0.1",
    "@ngx-translate/http-loader": "4.0.0",
    "rxjs": "6.5.2",
    "tslib": "1.10.0",
    "zone.js": "0.9.1"
  },
  "devDependencies":   {
    "@angular-devkit/build-angular": "0.802.0",
    "@angular/cli": "8.2.0",
    "@angular/compiler-cli": "8.2.0",
    "@angular/language-service": "8.2.0",
    "@commitlint/cli": "8.1.0",
    "@commitlint/config-angular": "8.1.0",
    "@phenomnomnominal/angular-lazy-routes-fix": "0.1.5",
    "@types/jasmine": "3.3.13",
    "@types/jasminewd2": "2.0.6",
    "@types/node": "12.0.8",
    "codelyzer": "5.1.0",
    "concurrently": "4.1.0",
    "http-server": "0.11.1",
    "husky": "4.2.2",
    "jasmine-core": "3.4.0",
    "jasmine-spec-reporter": "4.2.1",
    "jwt-decode": "2.2.0",
    "karma": "4.1.0",
    "karma-chrome-launcher": "2.2.0",
    "karma-cli": "2.0.0",
    "karma-coverage": "1.1.2",
    "karma-coverage-istanbul-reporter": "2.0.5",
    "karma-jasmine": "2.0.1",
    "karma-jasmine-html-reporter": "1.4.2",
    "karma-junit-reporter": "1.2.0",
    "karma-mocha-reporter": "2.2.5",
    "karma-remap-coverage": "0.1.5",
    "karma-sourcemap-loader": "0.3.7",
    "path": "0.12.7",
    "protractor": "5.4.2",
    "rimraf": "2.6.3",
    "serve-favicon": "2.5.0",
    "stylelint": "10.1.0",
    "ts-node": "8.3.0",
    "tslint": "5.17.0",
    "tslint-eslint-rules": "5.4.0",
    "typescript": "3.4.5"
  },
  "husky": {"hooks":   {
    "pre-commit": "npm run lint",
    "pre-push": "npm run build:prod && npm run test",
    "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
  }}
}